{
    "name": "docsify-project",
    "version": "1.0.0",
    "scripts": {
    "build": "rm -f docs/_sidebar.md && docsify generate docs && mkdir -p dist && cp -r docs/* dist/",
    "deploy": "gh-pages -d dist -b deploy-docs -r https://$GH_TOKEN@github.com/$GITHUB_REPOSITORY.git"
    },
    "dependencies": {
      "docsify-cli": "^4",
      "docsify": "^4.13.1",
      "gh-pages": "^6"
    }
  }
  
